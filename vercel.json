{
    "version": 2,
    "builds": [
        {
            "src": "package.json",
            "use": "vercel-sapper"
        },
        {
            "src": "app.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "app.ts"
        }
    ],
    "installCommand": "yarn install"
}